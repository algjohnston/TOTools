<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TOTools.Seeding.DoubleElimPage">
    <ContentPage.Content>

        <ScrollView Margin="16">
            <StackLayout>

                <Label Text="Winner's Bracket"
                       FontSize="Large"
                       Margin="0,0,0,16" />
                <ContentView x:Name="WinnersBracket">
                </ContentView>

                <Label Text="Loser's Bracket"
                       FontSize="Large"
                       Margin="0,0,0,16" />
                <ContentView x:Name="LosersBracket">
                </ContentView>

            </StackLayout>
        </ScrollView>

    </ContentPage.Content>
</ContentPage>