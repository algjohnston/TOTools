<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:eventMap="clr-namespace:CS341Project.EventMap"
             xmlns:models="clr-namespace:CS341Project.Models"
             xmlns:eventMapMap="clr-namespace:CS341Project.EventMapMap"
             x:Class="CS341Project.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <eventMapMap:RegionToStringConverter x:Key="RegionToStringConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
