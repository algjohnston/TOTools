<?xml version="1.0" encoding="utf-8"?>

<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CS341Project.AppEntry.HomePage"
             xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
             xmlns:eventMap="clr-namespace:CS341Project.EventMap"
             xmlns:thumbGen="clr-namespace:CS341Project.ThumbGen"
             xmlns:scheduler="clr-namespace:CS341Project.Scheduler"
             xmlns:seeding="clr-namespace:CS341Project.Seeding"
             android:TabbedPage.ToolbarPlacement="Bottom"
             android:TabbedPage.IsSwipePagingEnabled="false">
    <TabbedPage.Children>
        <NavigationPage Title="Seeding">
            <x:Arguments>
                <seeding:SeedingTabbedPage/>
            </x:Arguments>
        </NavigationPage>
        <NavigationPage Title="Scheduler">
            <x:Arguments>
                <scheduler:SchedulerEventPage/>
            </x:Arguments>
        </NavigationPage>        

        <NavigationPage Title="Map">
            <x:Arguments>
                <eventMap:EventMapPage/>
            </x:Arguments>
        </NavigationPage>   
        <NavigationPage Title="Thumb Gen">
            <x:Arguments>
                <thumbGen:ThumbGenPage/>
            </x:Arguments>
        </NavigationPage>
    </TabbedPage.Children>
</TabbedPage>